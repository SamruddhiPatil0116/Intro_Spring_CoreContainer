<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
 http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean class="com.prowings.core.beanlifecycle.Shape" init-method="myCustomInit" destroy-method="myCustomDestroy" name="shape1">
		<property name="length" value="60" />
		<property name="width" value="30" />
	</bean>
 
	 <bean class="com.prowings.core.beanlifecycle.Rectangle" init-method="myCustomInit" destroy-method="myCustomDestroy"  name="shape2">
		<property name="length" value="90" />
		<property name="width" value="10" />
	</bean> 
	
</beans>